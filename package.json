{
  "name": "postcss-switch",
  "version": "0.1.4",
  "description": "PostCSS plugin to switch code in CSS files",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "switch",
    "node modules",
    "npm",
    "css"
  ],
  "author": "Dimitris Chatzis <dim.chatzis1993@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimChtz/postcss-switch"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  }
}
